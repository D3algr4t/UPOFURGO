			<field name="name">empresaRepartidoras</field>
			<field name="res_model">empresaRepartidora</field>
			<field name="view_type">form</field>
			<field name="view_mode">form,tree</field>
		</record>
		<menuitem id="empresaRepartidoras_submenu" name="empresaRepartidoras" parent="upofurgo" />
		<menuitem id="ver_empresaRepartidoras_submenu" name="Ver empresaRepartidoras" parent="empresaRepartidoras_submenu" 
			action="act_empresaRepartidoras" />
	</data>
</openerp> 